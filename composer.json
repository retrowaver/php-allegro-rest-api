{
    "name": "retrowaver/allegro-rest-api",
    "description": "Simple PHP interface for Allegro REST API. Forked from github.com/Wiatrogon/php-allegro-rest-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Piotr Jakubik",
            "email": "wiatrogon@gmail.com"
        },
        {
            "name": "retrowaver"
        }
    ],
    "keywords" : [
        "allegro",
        "rest",
        "php"
    ],
    "require": {
        "php": ">=7.2",
        "psr/http-message": "^1.0",
        "php-http/client-implementation": "^1.0",
        "php-http/httplug": "^2.0",
        "php-http/message-factory": "^1.0",
        "php-http/discovery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Allegro\\REST\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "php-http/mock-client": "^1.0",
        "php-http/message": "^1.0",
        "guzzlehttp/psr7": "^1.0"
    }
}
